<h1>New invoice_item</h1>

<% form_for(@invoice_item) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :process_item_id %><br />
    <%= f.text_field :process_item_id %>
  </p>
  <p>
    <%= f.label :process_item_type %><br />
    <%= f.text_field :process_item_type %>
  </p>
  <p>
    <%= f.label :invoice_id %><br />
    <%= f.text_field :invoice_id %>
  </p>
  <p>
    <%= f.label :price %><br />
    <%= f.text_field :price %>
  </p>
  <p>
    <%= f.label :remarks %><br />
    <%= f.text_field :remarks %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', invoice_items_path %>